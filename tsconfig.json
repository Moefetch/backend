{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ES2017",
    "moduleResolution": "node",
    "outDir": "dist",
    "sourceMap": true,
    "declaration": true,
    "module": "commonjs",
    "baseUrl": "./",
    "forceConsistentCasingInFileNames": true
  },
  "lib": [
    "es5", "es6", "dom", "dom.iterable"
  ]
}