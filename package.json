{
  "name": "moefetch-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "ts-node makeSureMongoDbIsRunning.ts || start_mongodb.bat.lnk & ts-node index.ts",
    "buildWin": "tsc -p tsconfig.json & copy package.json dist & cd dist & pkg package.json & xcopy src\\Logic dist\\src\\Logic /i /s",
    "buildLinux": "tsc -p tsconfig.json; cp package.json dist/package.json; cd dist; pkg package.json; mkdir dist/src; cp -r src/Logic dist/src/Logic"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/nedb": "^1.8.12",
    "@types/node": "^16.11.9",
    "@types/uuid": "^8.3.4"
  },
  "dependencies": {
    "@types/probe-image-size": "^7.2.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "file-type": "^16.5.3",
    "form-data": "^4.0.0",
    "mongoose": "^6.0.13",
    "multer": "^1.4.4",
    "nedb": "^1.8.0",
    "probe-image-size": "^7.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2"
  },
  "pkg": {
    "scripts": [
      "./plugins/logicModels/*.js"
    ],
    "assets": [
      "./plugins/logicModels/*.js"
    ],
    "targets": [
      "latest-linux-x64",
      "latest-win-x64"
    ],
    "outputPath": "dist"
  },
  "bin": "./index.js"
}
